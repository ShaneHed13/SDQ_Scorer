<!DOCTYPE html>
    <html lang="en">
        
        <head>
            <title>DB Info</title>
            <meta charset="UTF-8">
        </head>
        
        <body>
        <h1>SDQ 4-10 Parent/Teacher Version</h1>
            <table><tr><th>Number</th><th>Question</th><th>Not True</th><th>Somewhat True</th><th>Always True</th></tr>
                
                
            <% sdqTests.forEach(function(r) { %>
                <tr><td><%= r.id %></td><td><%= r.sdq410pt %></td><td><input type="radio" name="<%= r.id %>"><%= r.never %></td><td><input type="radio" name="<%= r.id %>"><%= r.sometimes %></td><td><input type="radio" name="<%= r.id %>"><%= r.always %></td></tr>
            <% }); %>   
                
            </table>
        </body>

    </html>